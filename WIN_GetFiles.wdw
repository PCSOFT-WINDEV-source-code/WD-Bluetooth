         //Directory chosen by the user to retrieve the files
         sCopyDir is string = fSelectDir("", "Select the directory for file copy...", "")
         n is int 
         
         IF sCopyDir <> "" THEN 
         	
         	HourGlass (True)
         	
         	//Retrieve each one of the files selected in the table
         	FOR ALL SELECTED ROW n of TABLE_Files
         		
         		IF NOT OBEXGetFile(gnIDOBEXCnt, EDT_HomeDir + ["\"] + COL_File[n], sCopyDir + ["\"] + COL_File[n]) THEN
