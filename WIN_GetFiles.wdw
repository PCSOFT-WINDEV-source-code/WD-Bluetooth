        internal_properties : CAAAAAgAAADx2DCwXbsYW6eA/SOMoBS9wAoIDjPmM1XLlAr4hPvnJ5SBJTUq2XdLbJtseavGzc4DzxVrPscsN4JP08gZtWvmPKCWHHNgfgSkXSsOfqJdJII/atKMQM5hLK3sBFj+kbskWD+DrQh8L3n8O1Dt4A5moVhYCAKryJyR4kafhTN+/Q==
        code : |1+
         HourGlass(True)
         
         //Security code asked on the Bluetooth device that receives the file
         sSecurityCode is string = "0000"
         
         sDevice is string = COMBO_BTDevices[COMBO_BTDevices]
         
         // Bluetooth connection
         BTConnectDevice(gaaBTDevice[sDevice]:nDeviceID, sSecurityCode) 
         
         IF gnIDOBEXCnt = 0 THEN
         	//OBEX connection
         	gnIDOBEXCnt  = OBEXConnect(obexBluetooth, gaaBTDevice[sDevice]:sMACAddress)	
         END
         
         //A message is displayed on the screen of the mobile device to accept (or not) the file transfer
         Info("A message may be displayed on the screen of your mobile device, don't forget to validate it.")
         
         //Lists the files found in the directory
         sListDirectories is string = OBEXListFile(gnIDOBEXCnt,EDT_HomeDir)
         
         sADirectory is string 
         sFileName is string
         //Adds each file found to the table
         FOR EACH string sADirectory of sListDirectories SEPAREE by CR
         	sFileName = AnsiToUnicode(ExtractString(sADirectory, 1, TAB),charsetUTF8)
         	TableAddLine(TABLE_Files,sFileName)
         END
         
         HourGlass(False)
         
         //Message if no file was found 
         IF TABLE_Files..Occurrence = 0 THEN
         	Info("No file was found in the specified directory.") 
         END
         
